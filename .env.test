# Test configuration - for terminal testing without Google Sheets and n8n

# IMPORTANT: Use mock storage for testing
USE_MOCK_STORAGE=true

# Telegram API (not needed for mock testing, but required by config)
API_ID=12345678
API_HASH=test_hash_not_used_in_mock_mode

# n8n webhooks (optional, not used in mock mode)
N8N_WEBHOOK_INCOMING=
N8N_WEBHOOK_STATUS=
N8N_WEBHOOK_ACCOUNT=

# Google Sheets (not used in mock mode)
GOOGLE_SHEETS_ID=
GOOGLE_CREDENTIALS_FILE=

# Account Limits
MAX_DAILY_ACTIVE=7
MAX_DAILY_WARMING=3
COOLDOWN_HOURS=24

# Proxy (can test without actual proxy)
PROXY_ENABLED=false
DEFAULT_PROXY_TYPE=socks5
DEFAULT_PROXY_HOST=
DEFAULT_PROXY_PORT=
DEFAULT_PROXY_USER=
DEFAULT_PROXY_PASS=

# Server
HOST=0.0.0.0
PORT=8000
